      if (f_RemoveTrap_tryActivateTrap) //checks wheather the player successfully removed the trap, the condition succeed if player doesnt
        if !(strlen(<tag(spActivateChance)>)) //
          tag(spActivateChance,300)
        endif
        if !(<chance(<tag(spActivateChance)>,2)>)  //the bigger the tag spActivateChance is, the bigger the chance that the spawn remains active and that this condition doesn't succeed
          return 1
        endif
        if (safe.tag(restime))
          tag(spDeactivatedTill,<eval (<serv.time>+<tag(restime)>)>)
        else
          tag(restime,450)
          tag(spDeactivatedTill,<eval (<serv.time>+<tag(restime)>)>)  //defaultni prodleva mezi spawny
        endif
        more1=1 //docasne deact.
        src.newNPC(<TAG.spawnID>)
        src.act.newitem(i_zmizni)
        if !(strlen(<tag(npcDisappearTime)>))
          tag(npcDisappearTime,900)
        endif
        src.act.act.timer=<tag(npcDisappearTime)>
        src.act.equip <src.act.act>
        f_trap_moveTrap
      endif
    endif
  endif

ON=@userclick
  if (src.isGM)
    message("<findres(chardef,tag.spawnID).defname> (<findres(chardef,tag.spawnID).name>)")
    message("Prodleva: <tag(restime)>")
    message("<eval(((<tag.spDeactivatedTill>)-(<serv.time>))/10)>s do dalsi aktivace") // nevim, co s tim je, ale nejak to nechce pocitat, pokud tam nenarvu vsechny ty zavorky
    return 1
  endif

ON=@userdclick
  if (src.isGM)
    dialog(d_spawnpad)
    return 1
  endif

